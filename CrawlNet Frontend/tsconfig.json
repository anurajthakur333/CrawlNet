{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": false,
    "types": ["vite/client"]
  },
  "include": ["src"]
}
